<p id="notice"><%= notice %></p>

<h1>Submissions</h1>

<table>
  <thead>
    <tr>
      <th>Assessment</th>
      <th>Userid</th>
      <th>Useremail</th>
      <th>Scores</th>
      <th>Answers</th>
      <th>Submittedat</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @submissions.each do |submission| %>
      <tr>
        <td><%= submission.assessment_id %></td>
        <td><%= submission.userId %></td>
        <td><%= submission.userEmail %></td>
        <td><%= submission.Scores %></td>
        <td><%= submission.Answers %></td>
        <td><%= submission.SubmittedAt %></td>
        <td><%= link_to 'Show', submission %></td>
        <td><%= link_to 'Edit', edit_submission_path(submission) %></td>
        <td><%= link_to 'Destroy', submission, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Submission', new_submission_path %>
